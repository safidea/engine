{
  "name": "@latechforce/engine",
  "description": "Generate web app with a configuration file",
  "version": "0.34.41",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "Thomas JEANNEAU",
  "license": "BSL 1.1",
  "exports": {
    ".": "./dist/index.js",
    "./instrument": "./dist/infrastructure/instrument/index.js",
    "./schema/*": "./schema/*"
  },
  "files": [
    "dist/**/*",
    "schema/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/latechforce/engine.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/latechforce/engine/issues"
  },
  "homepage": "https://github.com/latechforce/engine#readme",
  "scripts": {
    "format": "bunx prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "lint": "bunx eslint . --fix",
    "prebuild": "bunx puppeteer browsers install chrome",
    "build:server": "bun run script/server/build.ts",
    "build:client": "bun run script/client/build.ts",
    "build:types": "bun run script/types/build.ts",
    "build:schema": "bun run script/schema/build.ts",
    "build": "bun run build:client && bun run build:server && bun run build:types && bun run build:schema",
    "watch:server": "nodemon --config script/server/watch.json",
    "watch:client": "nodemon --config script/client/watch.json",
    "watch:types": "nodemon --config script/types/watch.json",
    "watch:schema": "nodemon --config script/schema/watch.json",
    "watch": "concurrently \"bun run watch:schema\" \"bun run watch:client\" \"bun run watch:server\" \"bun run watch:types\"",
    "pretest": "bunx playwright install --with-deps",
    "test": "bun run test:unit && bunx playwright test",
    "test:unit": "bun test ./test/unit/**/*.test.ts --bail",
    "test:integration": "bunx playwright test test/integration",
    "test:e2e": "bunx playwright test test/e2e",
    "test:ui": "bunx playwright test --ui",
    "test:snapshots": "bunx playwright test --update-snapshots",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.0",
    "@heroicons/react": "^2.2.0",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo": "^8.0.12",
    "@ngrok/ngrok": "^1.4.1",
    "@notionhq/client": "^2.2.15",
    "@sentry/node": "^8.47.0",
    "@sentry/profiling-node": "^8.47.0",
    "@types/lodash": "^4.17.13",
    "adm-zip": "^0.5.16",
    "airtable": "^0.12.2",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.3",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "fs-extra": "^11.2.0",
    "googleapis": "^144.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "html-react-parser": "^5.2.1",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "marked": "^14.1.4",
    "nanoid": "^5.0.9",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "pg-boss": "^10.1.5",
    "postcss": "^8.4.49",
    "puppeteer": "^23.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.49.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testcontainers/postgresql": "^10.16.0",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/dompurify": "^3.2.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mailparser": "^3.4.5",
    "@types/node": "^22.10.2",
    "@types/nodemon": "^1.19.6",
    "@types/pg": "^8.11.10",
    "@types/react-dom": "^18.3.5",
    "@types/smtp-server": "^3.5.10",
    "@types/xml2js": "^0.4.14",
    "concurrently": "^9.1.0",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mammoth": "^1.8.0",
    "nodemon": "^3.1.9",
    "pdfreader": "^3.0.6",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0",
    "storybook": "^8.4.7",
    "typescript-eslint": "^8.18.1",
    "typescript-json-schema": "^0.65.1"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "bun": "1.1.42",
    "node": "22.12.0",
    "npm": "10.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "overrides": {
    "whatwg-url": "^14.0.0"
  }
}
